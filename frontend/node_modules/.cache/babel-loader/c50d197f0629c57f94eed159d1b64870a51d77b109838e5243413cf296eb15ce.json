{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BarraNavegacao\n  },\n  data() {\n    return {\n      usuario: null,\n      user: null\n    };\n  },\n  mounted() {\n    this.getUsuario(this.$route.params.id);\n  },\n  methods: {\n    getUsuario(id) {\n      axios({\n        method: \"GET\",\n        url: `http://localhost:8080/users/${id}`\n      }).then(r => {\n        this.usuario = r.data;\n        this.user = r.data;\n      });\n    },\n    atualizaUsuario() {\n      if (typeof this.usuario.permissoes == \"string\") {\n        this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n      }\n      if (typeof this.user.permissoes == \"string\") {\n        this.user.permissoes = this.user.permissoes.split(\",\");\n      }\n      axios({\n        method: \"PUT\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        },\n        data: JSON.stringify(this.usuario)\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    },\n    excluiUsuario() {\n      axios({\n        method: \"DELETE\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        }\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["BarraNavegacao","AuthService","axios","components","data","usuario","user","mounted","getUsuario","$route","params","id","methods","method","url","then","r","atualizaUsuario","permissoes","split","headers","Authorization","dados","token","JSON","stringify","console","log","$router","push","excluiUsuario"],"sources":["/home/nomade00/Área de Trabalho/Projeto-Aplicacao-Hibrida-main/demomongosec-master/frontend/src/views/usuarios/UsuarioEditView.vue"],"sourcesContent":["<template>\n    <barra-navegacao></barra-navegacao>\n\n    <div class=\"cadastro\">\n        <div class=\"row g-0 justify-content-center\"> <!-- Centraliza o conteúdo horizontalmente -->\n            <!-- Coluna responsiva -->\n            <div class=\"col-lg-6 col-md-6 col-sm-8\"> <!-- Ajusta o tamanho da coluna em diferentes dispositivos -->\n                <!-- Card de login -->\n                <div v-if=\"!registerActive\" class=\"card login\" v-bind:class=\"{ error: emptyFields }\">\n                    <h1>Usuário:</h1>\n                    <fieldset v-if=\"usuario != null\">\n                        <legend>Dados do usuário:</legend>\n                        <form class=\"\">\n                            <p>Login: {{ usuario.login }}</p>\n                            <div class=\"row gx-3 gy-2 align-items-center p-2\">\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"usuario.nome\" type=\"text\" class=\"form-control\" placeholder=\"Nome\"\n                                        required>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"user.sobrenome\" type=\"text\" class=\"form-control\"\n                                        placeholder=\"Sobrenome\" required>\n                                </div>\n                            </div>\n                            <div class=\"row gx-3 gy-2 align-items-center p-2\">\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"user.periodo\" type=\"text\" class=\"form-control\"\n                                        placeholder=\"Período\" required>\n                                </div>\n                                <p>Permissões:</p>\n                                <textarea v-model=\"user.permissoes\"></textarea>\n                                <hr />\n                            </div>\n                            <div class=\"row col-sm-6 align-items-center p-0\">\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary atualizar\"\n                                        @click=\"atualizaUsuario\">Atualizar</button>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary excluir\" @click=\"excluiUsuario\">Excluir</button>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary voltar\" onclick=\"history.back()\">Voltar</button>\n                                </div>\n                            </div>\n                        </form>\n                    </fieldset>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\n\nexport default {\n    components: {\n        BarraNavegacao,\n    },\n    data() {\n        return {\n            usuario: null,\n            user: null,\n        };\n    },\n    mounted() {\n        this.getUsuario(this.$route.params.id);\n    },\n    methods: {\n        getUsuario(id) {\n            axios({\n                method: \"GET\",\n                url: `http://localhost:8080/users/${id}`,\n            }).then((r) => {\n                this.usuario = r.data;\n                this.user = r.data;\n            });\n        },\n        atualizaUsuario() {\n            if (typeof this.usuario.permissoes == \"string\") {\n                this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n            }\n            if (typeof this.user.permissoes == \"string\") {\n                this.user.permissoes = this.user.permissoes.split(\",\");\n            }\n            axios({\n                method: \"PUT\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n                data: JSON.stringify(this.usuario),\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n        excluiUsuario() {\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n    },\n};\n</script>\n\n<style scoped>\n.card {\n    padding: 20px;\n    /* Define o espaçamento interno */\n}\n\nfieldset {\n    border: 2px solid blue;\n    /* Ajuste a borda conforme necessário */\n    padding: 20px;\n    /* Ajuste o espaçamento interno conforme necessário */\n    border-radius: 10px;\n    /* Ajuste o arredondamento dos cantos conforme necessário */\n    margin-top: 20px;\n    /* Ajuste a margem superior conforme necessário */\n}\n\nlegend {\n    font-size: 1.2em;\n    /* Ajuste o tamanho da fonte conforme necessário */\n    font-weight: bold;\n    /* Deixe o texto em negrito */\n}\n\ntextarea {\n    width: 100%;\n    /* Faz com que o textarea ocupe a largura total */\n    height: 100px;\n    /* Ajuste a altura conforme necessário */\n    margin-top: 10px;\n    /* Ajuste a margem superior conforme necessário */\n    border-radius: 5px;\n    /* Ajuste o arredondamento dos cantos conforme necessário */\n    padding: 10px;\n    /* Ajuste o espaçamento interno conforme necessário */\n}\n\n.atualizar:hover {\n    color: black;\n    background-color: yellow;\n}\n\n.excluir:hover {\n    color: black;\n    background-color: rgb(255, 21, 0);\n}\n\n.voltar:hover {\n    color: black;\n    background-color: rgb(71, 186, 0);\n}\n\nbutton {\n    /* Ajuste o tamanho da fonte conforme necessário */\n    box-shadow: 1px 2px 1px 1px rgba(0, 0, 0, 0.745) !important;\n}\n</style>\n"],"mappings":";AAsDA,OAAOA,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,WAAU,MAAO,wBAAwB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,IAAI;MACbC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;EAC1C,CAAC;EACDC,OAAO,EAAE;IACLJ,UAAUA,CAACG,EAAE,EAAE;MACXT,KAAK,CAAC;QACFW,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8BH,EAAG;MAC3C,CAAC,CAAC,CAACI,IAAI,CAAEC,CAAC,IAAK;QACX,IAAI,CAACX,OAAM,GAAIW,CAAC,CAACZ,IAAI;QACrB,IAAI,CAACE,IAAG,GAAIU,CAAC,CAACZ,IAAI;MACtB,CAAC,CAAC;IACN,CAAC;IACDa,eAAeA,CAAA,EAAG;MACd,IAAI,OAAO,IAAI,CAACZ,OAAO,CAACa,UAAS,IAAK,QAAQ,EAAE;QAC5C,IAAI,CAACb,OAAO,CAACa,UAAS,GAAI,IAAI,CAACb,OAAO,CAACa,UAAU,CAACC,KAAK,CAAC,GAAG,CAAC;MAChE;MACA,IAAI,OAAO,IAAI,CAACb,IAAI,CAACY,UAAS,IAAK,QAAQ,EAAE;QACzC,IAAI,CAACZ,IAAI,CAACY,UAAS,GAAI,IAAI,CAACZ,IAAI,CAACY,UAAU,CAACC,KAAK,CAAC,GAAG,CAAC;MAC1D;MACAjB,KAAK,CAAC;QACFW,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8B,IAAI,CAACT,OAAO,CAACM,EAAG,EAAC;QACrDS,OAAO,EAAE;UACLC,aAAa,EAAG,UAASpB,WAAW,CAACqB,KAAK,CAACC,KAAM,EAAC;UAClD,cAAc,EAAE;QACpB,CAAC;QACDnB,IAAI,EAAEoB,IAAI,CAACC,SAAS,CAAC,IAAI,CAACpB,OAAO;MACrC,CAAC,CAAC,CAACU,IAAI,CAAEC,CAAC,IAAK;QACXU,OAAO,CAACC,GAAG,CAACX,CAAC,CAAC;QACd,IAAI,CAACY,OAAO,CAACC,IAAI,CAAC,WAAW;MACjC,CAAC,CAAC;IACN,CAAC;IACDC,aAAaA,C