{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BarraNavegacao\n  },\n  data() {\n    return {\n      professor: {\n        nome: \"\",\n        email: \"\",\n        disciplina: \"\"\n      }\n    };\n  },\n  mounted() {\n    this.getProfessor();\n  },\n  methods: {\n    async getProfessor() {\n      try {\n        const response = await axios.get(`http://localhost:8080/professores/${this.$route.params.id}`);\n        this.professor = response.data;\n      } catch (error) {\n        console.error(\"Erro ao buscar professor:\", error);\n      }\n    },\n    async atualizarProfessor() {\n      try {\n        await axios.put(`http://localhost:8080/professores/${this.$route.params.id}`, this.professor);\n        alert(\"Professor atualizado com sucesso!\");\n        this.$router.push(\"/professores\");\n      } catch (error) {\n        console.error(\"Erro ao atualizar professor:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["BarraNavegacao","axios","components","data","professor","nome","email","disciplina","mounted","getProfessor","methods","response","get","$route","params","id","error","console","atualizarProfessor","put","alert","$router","push"],"sources":["/home/nomade00/√Årea de Trabalho/ProjetoIntegrador_Unisales/frontend/src/views/professores/EditarProfessor.vue"],"sourcesContent":["<template>\n    <barra-navegacao></barra-navegacao>\n  \n    <div class=\"container mt-4\">\n      <h1>Editar Professor</h1>\n      <form @submit.prevent=\"atualizarProfessor\">\n        <div class=\"mb-3\">\n          <label for=\"nome\" class=\"form-label\">Nome</label>\n          <input v-model=\"professor.nome\" type=\"text\" id=\"nome\" class=\"form-control\" required />\n        </div>\n        <div class=\"mb-3\">\n          <label for=\"email\" class=\"form-label\">Email</label>\n          <input v-model=