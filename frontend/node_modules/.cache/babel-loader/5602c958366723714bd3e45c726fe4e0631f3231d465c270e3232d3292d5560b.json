{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BarraNavegacao\n  },\n  data() {\n    return {\n      usuario: null,\n      permissoes: [{\n        label: \"Administrador\",\n        role: \"ROLE_ADMIN\"\n      }, {\n        label: \"Professor\",\n        role: \"ROLE_PROFESSOR\"\n      }, {\n        label: \"Usuário\",\n        role: \"ROLE_USER\"\n      }, {\n        label: \"Projetos\",\n        role: \"ROLE_PROJETOS\"\n      }]\n    };\n  },\n  mounted() {\n    this.getUsuario(this.$route.params.id);\n  },\n  methods: {\n    getUsuario(id) {\n      axios({\n        method: \"GET\",\n        url: `http://localhost:8080/users/${id}`\n      }).then(r => {\n        this.usuario = r.data;\n      });\n    },\n    atualizaUsuario() {\n      if (typeof this.usuario.permissoes === \"string\") {\n        this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n      }\n      axios({\n        method: \"PUT\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        },\n        data: JSON.stringify(this.usuario)\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    },\n    excluiUsuario() {\n      axios({\n        method: \"DELETE\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        }\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    },\n    voltar() {\n      this.$router.go(-1);\n    }\n  }\n};","map":{"version":3,"names":["BarraNavegacao","AuthService","axios","components","data","usuario","permissoes","label","role","mounted","getUsuario","$route","params","id","methods","method","url","then","r","atualizaUsuario","split","headers","Authorization","dados","token","JSON","stringify","console","log","$router","push","excluiUsuario","voltar","go"],"sources":["/home/nomade00/Área de Trabalho/Projeto-Aplicacao-Hibrida-main/demomongosec-master/frontend/src/views/usuarios/UsuarioEditView.vue"],"sourcesContent":["<template>\n    <barra-navegacao></barra-navegacao>\n  \n    <div class=\"cadastro\">\n      <div class=\"row g-0 justify-content-center\">\n        <!-- Centraliza o conteúdo horizontalmente -->\n        <!-- Coluna responsiva -->\n        <div class=\"col-lg-6 col-md-6 col-sm-8\">\n          <!-- Ajusta o tamanho da coluna em diferentes dispositivos -->\n          <!-- Card de login -->\n          <div v-if=\"!registerActive\" class=\"card login\" :class=\"{ error: emptyFields }\">\n            <h1>Usuário:</h1>\n            <fieldset v-if=\"usuario\">\n              <legend>Dados do usuário:</legend>\n              <form>\n                <div class=\"row gx-3 gy-2 align-items-center p-2\">\n                  <div class=\"col-sm-4 form-group\">\n                    <label>Login</label>\n                    <input v-model=\"usuario.login\" type=\"text\" class=\"form-control\" disabled />\n                  </div>\n                  <div class=\"col-sm-4 form-group\">\n                    <label>Nome</label>\n                    <input v-model=\"usuario.nome\" type=\"text\" class=\"form-control\" />\n                  </div>\n                  <div class=\"col-sm-4 form-group\">\n                    <label>Sobrenome</label>\n                    <input v-model=\"usuario.sobrenome\" type=\"text\" class=\"form-control\" />\n                  </div>\n                  <div class=\"col-sm-4 form-group\">\n                    <label>Período</label>\n                    <input v-model=\"usuario.periodo\" type=\"text\" class=\"form-control\" />\n                  </div>\n                  <div class=\"col-sm-8 menu-drop form-group\">\n                    <label>Permissões</label>\n                    <select v-model=\"usuario.permissoes\" multiple class=\"form-select\" aria-label=\"Permissões\">\n                      <option v-for=\"(permissao, index) in permissoes\" :key=\"index\" :value=\"permissao.role\">\n                        {{ permissao.label }}\n                      </option>\n                    </select>\n                  </div>\n                </div>\n                <div class=\"row gx-3 gy-2 col-sm-2 p-4 align-items-center\">\n                  <button class=\"btn btn-primary\" @click.prevent=\"atualizaUsuario\">Atualizar</button>\n                  <button class=\"btn btn-danger\" @click.prevent=\"excluiUsuario\">Excluir</button>\n                  <button class=\"btn btn-secondary\" @click=\"voltar\">Voltar</button>\n                </div>\n              </form>\n            </fieldset>\n          </div>\n        </div>\n      </div>\n    </div>\n  </template>\n  \n\n<script>\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\n\nexport default {\n    components: {\n        BarraNavegacao,\n    },\n    data() {\n        return {\n            usuario: null,\n            permissoes: [\n                { label: \"Administrador\", role: \"ROLE_ADMIN\" },\n                { label: \"Professor\", role: \"ROLE_PROFESSOR\" },\n                { label: \"Usuário\", role: \"ROLE_USER\" },\n                { label: \"Projetos\", role: \"ROLE_PROJETOS\" },\n            ],\n        };\n    },\n    mounted() {\n        this.getUsuario(this.$route.params.id);\n    },\n    methods: {\n        getUsuario(id) {\n            axios({\n                method: \"GET\",\n                url: `http://localhost:8080/users/${id}`,\n            }).then((r) => {\n                this.usuario = r.data;\n            });\n        },\n        atualizaUsuario() {\n            if (typeof this.usuario.permissoes === \"string\") {\n                this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n            }\n            axios({\n                method: \"PUT\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n                data: JSON.stringify(this.usuario),\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\");\n            });\n        },\n        excluiUsuario() {\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\");\n            });\n        },\n        voltar() {\n            this.$router.go(-1);\n        },\n    },\n};\n</script>\n\n<style scoped>\n.card {\n    padding: 20px;\n    /* Define o espaçamento interno */\n}\n\nfieldset {\n    border: 2px solid blue;\n    /* Ajuste a borda conforme necessário */\n    padding: 20px;\n    /* Ajuste o espaçamento interno conforme necessário */\n    border-radius: 10px;\n    /* Ajuste o arredondamento dos cantos conforme necessário */\n    margin-top: 20px;\n    /* Ajuste a margem superior conforme necessário */\n}\n\nlegend {\n    font-size: 1.2em;\n    /* Ajuste o tamanho da fonte conforme necessário */\n    font-weight: bold;\n    /* Deixe o texto em negrito */\n}\n\ntextarea {\n    width: 100%;\n    /* Faz com que o textarea ocupe a largura total */\n    height: 100px;\n    /* Ajuste a altura conforme necessário */\n    margin-top: 10px;\n    /* Ajuste a margem superior conforme necessário */\n    border-radius: 5px;\n    /* Ajuste o arredondamento dos cantos conforme necessário */\n    padding: 10px;\n    /* Ajuste o espaçamento interno conforme necessário */\n}\n\n.atualizar:hover {\n    color: black;\n    background-color: yellow;\n}\n\n.excluir:hover {\n    color: black;\n    background-color: rgb(255, 21, 0);\n}\n\n.voltar:hover {\n    color: black;\n    background-color: rgb(71, 186, 0);\n}\n\nbutton {\n    /* Ajuste o tamanho da fonte conforme necessário */\n    box-shadow: 1px 2px 1px 1px rgba(0, 0, 0, 0.745) !important;\n}\n</style>\n"],"mappings":";AAwDA,OAAOA,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,WAAU,MAAO,wBAAwB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE,IAAI;MACbC,UAAU,EAAE,CACR;QAAEC,KAAK,EAAE,eAAe;QAAEC,IAAI,EAAE;MAAa,CAAC,EAC9C;QAAED,KAAK,EAAE,WAAW;QAAEC,IAAI,EAAE;MAAiB,CAAC,EAC9C;QAAED,KAAK,EAAE,SAAS;QAAEC,IAAI,EAAE;MAAY,CAAC,EACvC;QAAED,KAAK,EAAE,UAAU;QAAEC,IAAI,EAAE;MAAgB,CAAC;IAEpD,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;EAC1C,CAAC;EACDC,OAAO,EAAE;IACLJ,UAAUA,CAACG,EAAE,EAAE;MACXX,KAAK,CAAC;QACFa,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8BH,EAAG;MAC3C,CAAC,CAAC,CAACI,IAAI,CAAEC,CAAC,IAAK;QACX,IAAI,CAACb,OAAM,GAAIa,CAAC,CAACd,IAAI;MACzB,CAAC,CAAC;IACN,CAAC;IACDe,eAAeA,CAAA,EAAG;MACd,IAAI,OAAO,IAAI,CAACd,OAAO,CAACC,UAAS,KAAM,QAAQ,EAAE;QAC7C,IAAI,CAACD,OAAO,CAACC,UAAS,GAAI,IAAI,CAACD,OAAO,CAACC,UAAU,CAACc,KAAK,CAAC,GAAG,CAAC;MAChE;MACAlB,KAAK,CAAC;QACFa,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8B,IAAI,CAACX,OAAO,CAACQ,EAAG,EAAC;QACrDQ,OAAO,EAAE;UACLC,aAAa,EAAG,UAASrB,WAAW,CAACsB,KAAK,CAACC,KAAM,EAAC;UAClD,cAAc,EAAE;QACpB,CAAC;QACDpB,IAAI,EAAEqB,IAAI,CAACC,SAAS,CAAC,IAAI,CAACrB,OAAO;MACrC,CAAC,CAAC,CAACY,IAAI,CAAEC,CAAC,IAAK;QACXS,OAAO,CAACC,GAAG,CAACV,CAAC,CAAC;QACd,IAAI,CAACW,OAAO,CAACC,IAAI,CAAC,W