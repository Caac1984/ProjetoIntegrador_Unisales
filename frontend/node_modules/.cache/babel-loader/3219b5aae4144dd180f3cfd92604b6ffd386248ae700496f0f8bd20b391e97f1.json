{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\n//import UtilService from \"@/services/UtilService\";\n\nimport axios from \"axios\";\nexport default {\n  components: {\n    BarraNavegacao\n  },\n  data() {\n    return {\n      usuario: null\n    };\n  },\n  mounted() {\n    this.getUsuario(this.$route.params.id);\n  },\n  methods: {\n    getUsuario(id) {\n      axios({\n        method: \"GET\",\n        url: `http://localhost:8080/users/${id}`\n      }).then(r => {\n        this.usuario = r.data;\n      });\n    },\n    atualizaUsuario() {\n      if (typeof this.usuario.permissoes == \"string\") {\n        this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n      }\n      axios({\n        method: \"PUT\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        },\n        data: JSON.stringify(this.usuario)\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    },\n    excluiUsuario() {\n      axios({\n        method: \"DELETE\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        }\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["BarraNavegacao","AuthService","axios","components","data","usuario","mounted","getUsuario","$route","params","id","methods","method","url","then","r","atualizaUsuario","permissoes","split","headers","Authorization","dados","token","JSON","stringify","console","log","$router","push","excluiUsuario"],"sources":["/home/nomade00/Área de Trabalho/Projeto-Aplicacao-Hibrida-main/demomongosec-master/frontend/src/views/usuarios/UsuarioEditView.vue"],"sourcesContent":["<template>\n    <barra-navegacao></barra-navegacao>\n\n    <div class=\"cadastro\">\n        <div class=\"row justify-content-center\"> <!-- Centraliza o conteúdo horizontalmente -->\n            <!-- Coluna responsiva -->\n            <div class=\"col-lg-6 col-md-6 col-sm-8\"> <!-- Ajusta o tamanho da coluna em diferentes dispositivos -->\n                <!-- Card de login -->\n                <div v-if=\"!registerActive\" class=\"card login\" v-bind:class=\"{ error: emptyFields }\">\n                    <h1>Usuário:</h1>\n                    <fieldset v-if=\"usuario != null\">\n                        <legend>Dados do usuário:</legend>\n                        <p>Login: {{ usuario.login }}</p>\n                        <p>Permissões:</p>\n                        <textarea v-model=\"usuario.permissoes\"></textarea>\n                        <hr />\n                        <button class=\"btn btn-primary salvar-btn\" @click=\"atualizaUsuario\">Atualiza</button> \n                        <button class=\"btn btn-primary salvar-btn\"\n                            @click=\"excluiUsuario\">Excluir</button><p></p>\n\n                    </fieldset>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\n//import UtilService from \"@/services/UtilService\";\n\nimport axios from \"axios\";\n\nexport default {\n    components: {\n        BarraNavegacao,\n    },\n    data() {\n        return {\n            usuario: null,\n        };\n    },\n    mounted() {\n        this.getUsuario(this.$route.params.id);\n    },\n    methods: {\n        getUsuario(id) {\n            axios({\n                method: \"GET\",\n                url: `http://localhost:8080/users/${id}`,\n            }).then((r) => {\n                this.usuario = r.data;\n            });\n        },\n        atualizaUsuario() {\n            if (typeof this.usuario.permissoes == \"string\") {\n                this.usuario.permissoes = this.usuario.permissoes.split(\",\");\n            }\n            axios({\n                method: \"PUT\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n                data: JSON.stringify(this.usuario),\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n        excluiUsuario() {\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n    },\n};\n</script>\n\n<style scoped>\n\nbutton{\n    margin: 0 10px;\n}\n\n</style>"],"mappings":";AA4BA,OAAOA,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,WAAU,MAAO,wBAAwB;AAChD;;AAEA,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;EAC1C,CAAC;EACDC,OAAO,EAAE;IACLJ,UAAUA,CAACG,EAAE,EAAE;MACXR,KAAK,CAAC;QACFU,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8BH,EAAG;MAC3C,CAAC,CAAC,CAACI,IAAI,CAAEC,CAAC,IAAK;QACX,IAAI,CAACV,OAAM,GAAIU,CAAC,CAACX,IAAI;MACzB,CAAC,CAAC;IACN,CAAC;IACDY,eAAeA,CAAA,EAAG;MACd,IAAI,OAAO,IAAI,CAACX,OAAO,CAACY,UAAS,IAAK,QAAQ,EAAE;QAC5C,IAAI,CAACZ,OAAO,CAACY,UAAS,GAAI,IAAI,CAACZ,OAAO,CAACY,UA