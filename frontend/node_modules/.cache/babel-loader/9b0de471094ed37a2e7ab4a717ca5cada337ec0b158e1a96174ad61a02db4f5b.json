{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    BarraNavegacao\n  },\n  data() {\n    return {\n      usuario: null\n    };\n  },\n  mounted() {\n    this.getUsuario(this.$route.params.id);\n  },\n  methods: {\n    getUsuario(id) {\n      axios({\n        method: \"GET\",\n        url: `http://localhost:8080/users/${id}`\n      }).then(r => {\n        this.usuario = r.data;\n      });\n    },\n    atualizaUsuario() {\n      axios({\n        method: \"PUT\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        },\n        data: JSON.stringify(this.usuario)\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    },\n    excluiUsuario() {\n      axios({\n        method: \"DELETE\",\n        url: `http://localhost:8080/users/${this.usuario.id}`,\n        headers: {\n          Authorization: `Bearer ${AuthService.dados.token}`,\n          \"Content-Type\": \"application/json\"\n        }\n      }).then(r => {\n        console.log(r);\n        this.$router.push(\"/usuarios\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["BarraNavegacao","AuthService","axios","components","data","usuario","mounted","getUsuario","$route","params","id","methods","method","url","then","r","atualizaUsuario","headers","Authorization","dados","token","JSON","stringify","console","log","$router","push","excluiUsuario"],"sources":["/home/nomade00/Área de Trabalho/Projeto-Aplicacao-Hibrida-main/demomongosec-master/frontend/src/views/usuarios/UsuarioEditView.vue"],"sourcesContent":["<template>\n    <barra-navegacao></barra-navegacao>\n\n    <div class=\"cadastro\">\n        <div class=\"row g-0 justify-content-center\"> <!-- Centraliza o conteúdo horizontalmente -->\n            <!-- Coluna responsiva -->\n            <div class=\"col-lg-6 col-md-6 col-sm-8\"> <!-- Ajusta o tamanho da coluna em diferentes dispositivos -->\n                <!-- Card de login -->\n                <div v-if=\"!registerActive\" class=\"card login\" v-bind:class=\"{ error: emptyFields }\">\n                    <h1>Usuário:</h1>\n                    <fieldset v-if=\"usuario != null\">\n                        <legend>Dados do usuário:</legend>\n                        <form class=\"\">\n                            <p>Login: {{ usuario.login }}</p>\n                            <div class=\"row gx-3 gy-2 align-items-center p-2\">\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"usuario.nome\" type=\"text\" class=\"form-control\" placeholder=\"Nome\"\n                                        required>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"usuario.sobrenome\" type=\"text\" class=\"form-control\"\n                                        placeholder=\"Sobrenome\" required>\n                                </div>\n                            </div>\n                            <div class=\"row gx-3 gy-2 align-items-center p-2\">\n                                <div class=\"col-sm-4\">\n                                    <input v-model=\"usuario.periodo\" type=\"text\" class=\"form-control\"\n                                        placeholder=\"Período\" required>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <p>Permissões:</p>\n                                    <div class=\"form-check\">\n                                        <input class=\"form-check-input\" type=\"checkbox\" id=\"adminCheck\" value=\"admin\"\n                                            v-model=\"usuario.permissoes\">\n                                        <label class=\"form-check-label\" for=\"adminCheck\">\n                                            Admin\n                                        </label>\n                                    </div>\n                                    <div class=\"form-check\">\n                                        <input class=\"form-check-input\" type=\"checkbox\" id=\"userCheck\" value=\"user\"\n                                            v-model=\"usuario.permissoes\">\n                                        <label class=\"form-check-label\" for=\"userCheck\">\n                                            User\n                                        </label>\n                                    </div>\n                                    <div class=\"form-check\">\n                                        <input class=\"form-check-input\" type=\"checkbox\" id=\"projetosCheck\" value=\"projetos\"\n                                            v-model=\"usuario.permissoes\">\n                                        <label class=\"form-check-label\" for=\"projetosCheck\">\n                                            Projetos\n                                        </label>\n                                    </div>\n                                    <hr />\n                                </div>\n                            </div>\n                            <div class=\"row col-sm-6 align-items-center p-0\">\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary atualizar\"\n                                        @click=\"atualizaUsuario\">Atualizar</button>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary excluir\" @click=\"excluiUsuario\">Excluir</button>\n                                </div>\n                                <div class=\"col-sm-4\">\n                                    <button class=\"btn btn-primary voltar\" onclick=\"history.back()\">Voltar</button>\n                                </div>\n                            </div>\n                        </form>\n                    </fieldset>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport BarraNavegacao from \"@/components/BarraNavegacao.vue\";\nimport AuthService from \"@/services/AuthService\";\nimport axios from \"axios\";\n\nexport default {\n    components: {\n        BarraNavegacao,\n    },\n    data() {\n        return {\n            usuario: null,\n        };\n    },\n    mounted() {\n        this.getUsuario(this.$route.params.id);\n    },\n    methods: {\n        getUsuario(id) {\n            axios({\n                method: \"GET\",\n                url: `http://localhost:8080/users/${id}`,\n            }).then((r) => {\n                this.usuario = r.data;\n            });\n        },\n        atualizaUsuario() {\n            axios({\n                method: \"PUT\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n                data: JSON.stringify(this.usuario),\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n        excluiUsuario() {\n            axios({\n                method: \"DELETE\",\n                url: `http://localhost:8080/users/${this.usuario.id}`,\n                headers: {\n                    Authorization: `Bearer ${AuthService.dados.token}`,\n                    \"Content-Type\": \"application/json\",\n                },\n            }).then((r) => {\n                console.log(r);\n                this.$router.push(\"/usuarios\")\n            });\n        },\n    },\n};\n</script>\n\n<style scoped>\n.card {\n    padding: 20px;\n    /* Define o espaçamento interno */\n}\n\nfieldset {\n    border: 2px solid blue;\n    /* Ajuste a borda conforme necessário */\n    padding: 20px;\n    /* Ajuste o espaçamento interno conforme necessário */\n    border-radius: 10px;\n    /* Ajuste o arredondamento dos cantos conforme necessário */\n    margin-top: 20px;\n    /* Ajuste a margem superior conforme necessário */\n}\n\nlegend {\n    font-size: 1.2em;\n    /* Ajuste o tamanho da fonte conforme necessário */\n    font-weight: bold;\n    /* Deixe o texto em negrito */\n}\n\nform-check-input {\n    /* Ajuste o estilo dos checkboxes conforme necessário */\n    margin-right: 5px; /* Adiciona margem à direita para separar os checkboxes */\n}\n\n.atualizar:hover {\n    color: black;\n    background-color: yellow;\n}\n\n.excluir:hover {\n    color: black;\n    background-color: rgb(255, 21, 0);\n}\n\n.voltar:hover {\n    color: black;\n    background-color: rgb(71, 186, 0);\n}\n\nbutton {\n    /* Ajuste o tamanho da fonte conforme necessário */\n    box-shadow: 1px 2px 1px 1px rgba(0, 0, 0, 0.745) !important;\n}\n</style>\n"],"mappings":";AA4EA,OAAOA,cAAa,MAAO,iCAAiC;AAC5D,OAAOC,WAAU,MAAO,wBAAwB;AAChD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,UAAU,CAAC,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;EAC1C,CAAC;EACDC,OAAO,EAAE;IACLJ,UAAUA,CAACG,EAAE,EAAE;MACXR,KAAK,CAAC;QACFU,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8BH,EAAG;MAC3C,CAAC,CAAC,CAACI,IAAI,CAAEC,CAAC,IAAK;QACX,IAAI,CAACV,OAAM,GAAIU,CAAC,CAACX,IAAI;MACzB,CAAC,CAAC;IACN,CAAC;IACDY,eAAeA,CAAA,EAAG;MACdd,KAAK,CAAC;QACFU,MAAM,EAAE,KAAK;QACbC,GAAG,EAAG,+BAA8B,IAAI,CAACR,OAAO,CAACK,EAAG,EAAC;QACrDO,OAAO,EAAE;UACLC,aAAa,EAAG,UAASjB,WAAW,CAACkB,KAAK,CAACC,KAAM,EAAC;UAClD,cAAc,EAAE;QACpB